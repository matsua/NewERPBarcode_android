<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >
    <LinearLayout
        android:layout_width="match_parent"
	    android:layout_height="30dp"
	    android:orientation="horizontal">
	 	<!-- √ -->
	    <CheckBox
		    android:id="@+id/ism_mnt_list_isChecked"
		    android:layout_width="40dp"
			android:layout_height="wrap_content"
			android:checked="false"
			android:padding="0dp"
			android:focusable="false"
			android:soundEffectsEnabled="true"
			android:focusableInTouchMode="true" />
	    <!-- 신규바코드 -->
	    <TextView
	        android:id="@+id/ism_mnt_newBarcode"
	        android:layout_width="160dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 훼손바코드 -->
	    <TextView
	        android:id="@+id/ism_mnt_injuryBarcode"
	        android:layout_width="160dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 자재코드 -->
	    <TextView
	        android:id="@+id/ism_mnt_itemCode"
	        android:layout_width="160dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 자재명 -->
	    <TextView
	        android:id="@+id/ism_mnt_itemName"
	        android:layout_width="100dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 품목구분 -->
	    <TextView
	        android:id="@+id/ism_mnt_itemCategoryName"
	        android:layout_width="100dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 부품종류 -->
	    <TextView
	        android:id="@+id/ism_mnt_partKindName"
	        android:layout_width="80dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 처리일 -->
	    <TextView
	        android:id="@+id/ism_mnt_transactionDate"
	        android:layout_width="80dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 처리시간 -->
	    <TextView
	        android:id="@+id/ism_mnt_transactionTime"
	        android:layout_width="80dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 진행상태 -->
	    <TextView
	        android:id="@+id/ism_mnt_transactionStatusName"
	        android:layout_width="80dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 장치바코드 -->
	    <TextView
	        android:id="@+id/ism_mnt_deviceId"
	        android:layout_width="100dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 위치코드 -->
	    <TextView
	        android:id="@+id/ism_mnt_locationCode"
	        android:layout_width="200dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 위치명 -->
	    <TextView
	        android:id="@+id/ism_mnt_locationName"
	        android:layout_width="500dp"
	        style="@style/Form.Styled.NormalTextView" />
	    
	    <!-- 자재분류(대/중/소/세) -->
	    <TextView
	        android:id="@+id/ism_mnt_itemClassificationName"
	        android:layout_width="450dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 자산분류(대/중/소/세) -->
	    <TextView
	        android:id="@+id/ism_mnt_assetClassificationName"
	        android:layout_width="450dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 제조사명 -->
	    <TextView
	        android:id="@+id/ism_mnt_makerName"
	        android:layout_width="200dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 제조사 S/N -->
	    <TextView
	        android:id="@+id/ism_mnt_makerSerial"
	        android:layout_width="160dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 처리자 -->
	    <TextView
	        android:id="@+id/ism_mnt_transactionUserName"
	        android:layout_width="80dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 요청사유 -->
	    <TextView
	        android:id="@+id/ism_mnt_publicationWhyName"
	        android:layout_width="80dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 운용조직 -->
	    <TextView
	        android:id="@+id/ism_mnt_orgName"
	        android:layout_width="500dp"
	        style="@style/Form.Styled.NormalTextView" />
    </LinearLayout>
</LinearLayout>
