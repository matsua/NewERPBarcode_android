<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical" >
    <LinearLayout
        android:layout_width="match_parent"
	    android:layout_height="30dp"
	    android:orientation="horizontal">
		<!-- 자재코드 -->
	    <TextView
	        android:id="@+id/selectproduct_list_matnr"
	        android:layout_width="100dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 자재명 -->
	    <TextView
	        android:id="@+id/selectproduct_list_maktx"
	        android:layout_width="400dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 품목구분 -->
	    <TextView
	        android:id="@+id/selectproduct_list_devTypeName"
	        android:layout_width="100dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 부품종류 -->
	    <TextView
	        android:id="@+id/selectproduct_list_partTypeName"
	        android:layout_width="100dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 자재분류 -->
	    <TextView
	        android:id="@+id/selectproduct_list_itemClassificationName"
	        android:layout_width="550dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 제조사 -->
	    <TextView
	        android:id="@+id/selectproduct_list_zemaft_name"
	        android:layout_width="350dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 자재유형 -->
	    <TextView
	        android:id="@+id/selectproduct_list_mtart"
	        android:layout_width="80dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 바코드라벨링여부 -->
	    <TextView
	        android:id="@+id/selectproduct_list_barcd"
	        android:layout_width="120dp"
	        style="@style/Form.Styled.NormalTextView" />
	</LinearLayout>
</LinearLayout>
