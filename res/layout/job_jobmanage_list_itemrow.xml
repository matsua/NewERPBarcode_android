<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
    android:layout_width="match_parent"
    android:layout_height="30dp"
    android:orientation="horizontal" >
	<!-- √ -->
    <CheckBox
	    android:id="@+id/jobmanage_list_checked"
	    android:layout_width="40dp"
		android:layout_height="wrap_content"
		android:checked="false"
		android:padding="0dp"
		android:focusable="false"
		android:soundEffectsEnabled="true"
		android:focusableInTouchMode="true" />
	<!-- 작업구분 -->
    <TextView
        android:id="@+id/jobmanage_list_workName"
        android:layout_width="100dp"
        style="@style/Form.Styled.NormalTextView" />
    <!-- 작업시간 -->
    <TextView
        android:id="@+id/jobmanage_list_inputTime"
        android:layout_width="200dp"
        style="@style/Form.Styled.NormalTextView" />
	<!-- 전송여부 -->
    <TextView
        android:id="@+id/jobmanage_list_tranYn"
        android:layout_width="80dp"
        style="@style/Form.Styled.NormalTextView" />
    <!-- 메시지 -->
    <TextView
        android:id="@+id/jobmanage_list_message"
        android:layout_width="300dp"
        style="@style/Form.Styled.NormalTextView" />
    <!-- 위치바코드 -->
    <TextView 
        android:id="@+id/jobmanage_list_locCd"
        android:layout_width="190dp"
        style="@style/Form.Styled.NormalTextView" />
    <!-- 장치아이디 -->
    <TextView 
        android:id="@+id/jobmanage_list_deviceId"
        android:layout_width="80dp"
        style="@style/Form.Styled.NormalTextView" />
    <!-- WBS번호 -->
    <TextView 
        android:id="@+id/jobmanage_list_wbsNo"
        android:layout_width="200dp"
        style="@style/Form.Styled.NormalTextView" />
	<!-- 음영지역작업 여부 -->
    <TextView 
        android:id="@+id/jobmanage_list_offlineYn"
        android:layout_width="120dp"
        style="@style/Form.Styled.NormalTextView" />

</LinearLayout>
