<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >
    <LinearLayout
        android:layout_width="match_parent"
	    android:layout_height="30dp"
	    android:orientation="horizontal">
	 	<!-- √ -->
	    <CheckBox
		    android:id="@+id/repair_list_isChecked"
		    android:layout_width="40dp"
			android:layout_height="wrap_content"
			android:checked="false"
			android:padding="0dp"
			android:focusable="false"
			android:soundEffectsEnabled="true"
			android:focusableInTouchMode="true" />
	    <!-- 설비바코드 -->
	    <TextView
	        android:id="@+id/repair_list_facCd"
	        android:layout_width="160dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 설비바S/N -->
	    <TextView
	        android:id="@+id/repair_list_snCd"
	        android:layout_width="160dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 설비상태 -->
	    <TextView
	        android:id="@+id/repair_list_fsstCd"
	        android:layout_width="100dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 자재코드 -->
	    <TextView
	        android:id="@+id/repair_list_productCode"
	        android:layout_width="100dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 자재명 -->
	    <TextView
	        android:id="@+id/repair_list_productName"
	        android:layout_width="400dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 품목구분 -->
	    <TextView
	        android:id="@+id/repair_list_devTypeName"
	        android:layout_width="80dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 부품종류 -->
	    <TextView
	        android:id="@+id/repair_list_partType"
	        android:layout_width="80dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 장치바코드 -->
	    <TextView
	        android:id="@+id/repair_list_fsbarCd"
	        android:layout_width="100dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 고장등록취소,수리의뢰취소 -->
	    <!-- 설비상태 -->
	    <TextView
	        android:id="@+id/repair_list_facStatus"
	        android:layout_width="100dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 고장코드 -->
	    <TextView
	        android:id="@+id/repair_list_failureCode"
	        android:layout_width="60dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 고장명 -->
	    <TextView
	        android:id="@+id/repair_list_failureName"
	        android:layout_width="160dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 협력사코드 -->
	    <TextView
	        android:id="@+id/repair_list_partnerCode"
	        android:layout_width="160dp"
	        style="@style/Form.Styled.NormalTextView" />
	   	<!-- 협력사명 -->
	    <TextView
	        android:id="@+id/repair_list_partnerName"
	        android:layout_width="260dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 고장등록번호 -->
	    <TextView
	        android:id="@+id/repair_list_failureRegNo"
	        android:layout_width="140dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 수리의뢰번호 -->
	    <TextView
	        android:id="@+id/repair_list_repairRequestNo"
	        android:layout_width="140dp"
	        style="@style/Form.Styled.NormalTextView" />
    	
    	<!-- 수리완료 -->
	    <TextView
	        android:id="@+id/repair_list_locCd"
	        style="@style/Form.Styled.NormalTextView"
	        android:layout_width="200dp"
	        android:text="위치바코드" />
	    <TextView
	        android:id="@+id/repair_list_locName"
	        style="@style/Form.Styled.NormalTextView"
	        android:layout_width="350dp"
	        android:text="위치명" />
	    <TextView
	        android:id="@+id/repair_list_URCODN"
	        style="@style/Form.Styled.NormalTextView"
	        android:layout_width="150dp"
	        android:text="원인유형" />
	    	    <TextView
	        android:id="@+id/repair_list_MNCODN"
	        style="@style/Form.Styled.NormalTextView"
	        android:layout_width="150dp"
	        android:text="수리유형" />
	    <TextView
	        android:id="@+id/repair_list_failureCode_2"
	        style="@style/Form.Styled.NormalTextView"
	        android:layout_width="60dp"
	        android:text="고장코드" />
	    <TextView
	        android:id="@+id/repair_list_repairRequestNo_2"
	        style="@style/Form.Styled.NormalTextView"
	        android:layout_width="140dp"
	        android:text="수리의뢰번호" />
	    <TextView
	        android:id="@+id/repair_list_injurybarcode"
	        style="@style/Form.Styled.NormalTextView"
	        android:layout_width="140dp"
	        android:text="기존바코드" />
	    <TextView
	        android:id="@+id/repair_list_rreason"
	        style="@style/Form.Styled.NormalTextView"
	        android:layout_width="140dp"
	        android:text="대체발행사유" />
	    <!-- // 수리완료 -->
	    <!-- // 고장등록취소,수리의뢰취소 -->

		<!-- 개조개량 -->	    	    
	    <!-- 장치바코드 -->
	    <TextView
	        android:id="@+id/repair_list_deviceid"
	        android:layout_width="100dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 지시서번호 -->
	    <TextView
	        android:id="@+id/repair_list_upgdoc"
	        android:layout_width="100dp"
	        style="@style/Form.Styled.NormalTextView" />
	    <!-- 운용조직 -->
	    <TextView
	        android:id="@+id/repair_list_orgInfo"
	        android:layout_width="240dp"
	        style="@style/Form.Styled.NormalTextView" />
    </LinearLayout>
</LinearLayout>
